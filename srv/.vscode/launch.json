    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "swagger_server",
                "FLASK_ENV": "development",
                "FLASK_PORT": "3000",
                "FLASK_DEBUG": "0"
            },
            "args": [
                "run",
                "--no-debugger",
                "--port",
                "3000"
            ],
            "jinja": true
        }
    ]
}